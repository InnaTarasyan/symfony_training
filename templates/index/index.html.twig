{% extends 'base.html.twig' %}

{% block title %}Inna Tarasyan HomePage!{% endblock %}

{% block stylesheets %}
    {{ parent() }}

    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/dt-1.10.13/datatables.min.css"/>
    <link href="{{ absolute_url(asset('css/datatables.css')) }}" rel="stylesheet" />
    <link href="{{ absolute_url(asset('css/modal.css')) }}" rel="stylesheet" />

{% endblock %}

{% block body %}

    {{ include('index/menu.html.twig') }}
    {{ include('index/home.html.twig') }}
    {{ include('index/aboutUs.html.twig') }}
    {{ include('index/education.html.twig') }}
    {{ include('index/portfolio.html.twig') }}
    {{ include('index/contact_us.html.twig') }}
{% endblock %}

{% block javascripts %}
    {{ parent() }}

    <script src="https://cdn.datatables.net/1.10.6/js/jquery.dataTables.min.js"></script>
    <script src="{{ absolute_url(asset('js/home.js')) }}"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key={{ key }}&callback=myMap"></script>

{% endblock %}
